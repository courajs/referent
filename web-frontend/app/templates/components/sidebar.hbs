<div ...attributes>
  {{#link-to 'new'}}
    <div class="text-center my-4">
      <button class="bg-blue hover:bg-blue-dark text-white py-2 px-4 rounded mx-auto">
        New page
      </button>
    </div>
  {{/link-to}}

  <h2>Pages</h2>
  <ul class="mt-4 mb-12">
    {{#each (subscribe this.graph.pages) key="uuid" as |page|}}
      <li>{{link-to (or (get (subscribe page.title) 'value') '<untitled>') 'page' page.uuid}}</li>
    {{else}}
      <p class="text-grey-darker">&lt;none&gt;</p>
    {{/each}}
  </ul>
</div>
